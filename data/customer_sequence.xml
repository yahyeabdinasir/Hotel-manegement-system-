<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="customer_sequence" model="ir.sequence">
        <field name="name">customer sequence</field>
        <field name="code">hotel.customer</field>
        <field name="padding">5</field>
        <field name="prefix">CTR</field>
        <field name="company_id" eval="False"/>


    </record>

     <record id="room_sequence" model="ir.sequence">
        <field name="name">Room sequence</field>
        <field name="code">hotel.room</field>
        <field name="padding">4</field>
        <field name="prefix">QOLKA</field>
        <field name="company_id" eval="False"/>


    </record>

</odoo>